{
  "name": "eventflow-analytics_attendance-tracker",
  "version": "1.0.0",
  "description": "Digital Attendance & Participation Tracker for College Events",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" \"npm run dev:mobile\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm start",
    "dev:mobile": "cd mobile && npm start",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install && cd ../mobile && npm install",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Venkatdinesh20/eventflow-analytics_attendance-tracker.git"
  },
  "keywords": [
    "attendance-tracking",
    "event-management",
    "college-events",
    "digital-transformation",
    "react-native",
    "nodejs",
    "mongodb"
  ],
  "authors": [
    "Venkat Dinesh Pasupuleti <venkat.pasupuleti@college.edu>",
    "Shalini Sirdhala <shalini.sirdhala@college.edu>",
    "Lokeshwar Reddy Tippani <lokeshwar.tippani@college.edu>"
  ],
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}